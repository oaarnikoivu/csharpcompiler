		getint(var i);
		if (n > 0) then
			if (n < MAX) then
				for i := 0 to 10 do
					begin
						putint(n); puteol();
						n := n * i 
					end
				next
			else
				if (n = MAX) then 
					begin 
						putint(n); puteol();
						n := n / i
					end
				else
					begin
						putint(n); puteol();
					end
				endif
			endif
